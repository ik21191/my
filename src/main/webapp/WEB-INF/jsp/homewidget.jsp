<div class="hpag_product_row">
    <h2>Best Seller <a class="flot-right" href="#">VIEW ENTIRE COLLECTION</a></h2>
    <c:forEach items="${widgetMap.list1}" var="r">
    <div class="hpag_product_colum">
    <a href="${r.productURL}"><img alt="flower" src="${r.smallImage}"/>
    <span class="product_name">${r.productName}</span>
    <span class="product_rate">${r.wp}</span> </a>      
